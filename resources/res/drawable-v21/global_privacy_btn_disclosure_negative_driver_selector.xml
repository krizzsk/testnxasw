<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="#14000000">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="6dp"/>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:id="@+id/ripple_drawable_content">
        <selector>
            <item android:state_enabled="false">
                <shape android:shape="rectangle">
                    <corners android:radius="6dp"/>
                    <stroke android:width="1dp" android:color="#cccccc"/>
                </shape>
            </item>
            <item android:state_enabled="true">
                <shape android:shape="rectangle">
                    <corners android:radius="6dp"/>
                    <stroke android:width="1dp" android:color="#333333"/>
                </shape>
            </item>
        </selector>
    </item>
</ripple>
