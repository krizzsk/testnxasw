<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/title_bar_layout_above" android:background="@drawable/dimina_common_titlebar_bg" android:clickable="true" android:layout_width="match_parent" android:layout_height="44dp">
        <com.didi.dimina.container.p065ui.title.ImageTextButton android:id="@+id/common_title_bar_left1_tv" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="11dp" android:layout_centerVertical="true"/>
        <com.didi.dimina.container.p065ui.title.ImageTextButton android:id="@+id/common_title_bar_left2_tv" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toRightOf="@+id/common_title_bar_left1_tv" android:layout_centerVertical="true"/>
        <com.didi.dimina.container.p065ui.title.ImageTextButton android:id="@+id/common_title_bar_left3_tv" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toRightOf="@+id/common_title_bar_left2_tv" android:layout_centerVertical="true"/>
        <TextView android:textSize="18sp" android:textColor="#333333" android:ellipsize="end" android:gravity="center" android:id="@+id/common_title_bar_middle_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="240dp" android:singleLine="true" android:layout_centerInParent="true" android:textAllCaps="false" android:breakStrategy="0"/>
        <com.didi.dimina.container.p065ui.title.ImageTextButton android:id="@+id/common_title_bar_right1_tv" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_centerVertical="true" android:layout_marginEnd="11dp" android:layout_alignParentEnd="true"/>
        <com.didi.dimina.container.p065ui.title.ImageTextButton android:id="@+id/common_title_bar_right2_tv" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_centerVertical="true" android:layout_toStartOf="@+id/common_title_bar_right1_tv"/>
        <com.didi.dimina.container.p065ui.title.ImageTextButton android:id="@+id/common_title_bar_right3_tv" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_centerVertical="true" android:layout_toStartOf="@+id/common_title_bar_right2_tv" style="@style/DiminaNavigationBarButton"/>
    </RelativeLayout>
    <ImageView android:id="@+id/common_title_bar_line" android:background="@drawable/dimina_common_titlebar_shadow" android:clickable="false" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_below="@+id/title_bar_layout_above"/>
</RelativeLayout>
