<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/fragment_container_view" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/contentView" android:background="@color/white" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
        <androidx.core.widget.NestedScrollView android:layout_width="match_parent" android:layout_height="match_parent" android:fillViewport="true" android:layout_above="@+id/wallet_account_appply_bottom" android:layout_alignParentTop="true">
            <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
                <TextView android:textSize="22sp" android:textStyle="bold" android:textColor="@color/oc_color_000000" android:id="@+id/tv_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="12dp" android:text="@string/Fintech_Payment_Optimization_Enter_additional_IDXK"/>
                <TextView android:textSize="14sp" android:textStyle="normal" android:textColor="#666666" android:id="@+id/tv_subTitle" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="16dp" android:layout_marginRight="18dp" android:text="@string/Fintech_Payment_Optimization_According_to_cSoy" android:layout_marginHorizontal="18dp"/>
                <LinearLayout android:orientation="vertical" android:id="@+id/income_range" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="20dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp">
                    <TextView android:textSize="@dimen/wallet_sp_12" android:textColor="#999999" android:id="@+id/income_desc" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/Fintech_Payment_Integration__uMtp"/>
                    <TextView android:textSize="@dimen/wallet_sp_20" android:textColor="#000000" android:id="@+id/income_text" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:hint="@string/Fintech_Payment_Integration__mmMS"/>
                    <View android:background="#1a000000" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginTop="10dp"/>
                </LinearLayout>
                <com.didi.payment.kycservice.widget.NewPixInputView android:id="@+id/account_zipcode" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="10dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp"/>
                <LinearLayout android:orientation="vertical" android:id="@+id/account_expand_info_container" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
                    <com.didi.payment.kycservice.widget.NewPixInputView android:id="@+id/account_street" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="10dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp"/>
                    <com.didi.payment.kycservice.widget.NewPixInputView android:id="@+id/account_apartment_number" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="10dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp"/>
                    <com.didi.payment.kycservice.widget.NewPixInputView android:id="@+id/account_other_info" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="10dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp"/>
                    <com.didi.payment.kycservice.widget.NewPixInputView android:id="@+id/account_district" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="10dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp"/>
                    <com.didi.payment.kycservice.widget.NewPixInputView android:id="@+id/account_state" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="10dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp"/>
                    <com.didi.payment.kycservice.widget.NewPixInputView android:id="@+id/account_city" android:layout_width="match_parent" android:layout_height="80dp" android:layout_marginTop="10dp" android:layout_marginStart="18dp" android:layout_marginEnd="18dp"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
        <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@+id/wallet_account_appply_bottom" android:background="@android:color/white" android:paddingTop="@dimen/wallet_dp_6" android:paddingBottom="@dimen/wallet_dp_6" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:paddingStart="@dimen/wallet_dp_6" android:paddingEnd="@dimen/wallet_dp_24" style="@style/wallet_kyc_shadow_ef">
            <ImageView android:id="@+id/btn_pre_page" android:background="@drawable/icon_kyc_bottom_back" android:visibility="gone" android:layout_width="60dp" android:layout_height="70dp"/>
            <View android:id="@+id/divider_view" android:visibility="gone" android:layout_width="@dimen/wallet_dp_18" android:layout_height="1dp"/>
            <TextView android:textColor="@drawable/pix_btn_text_status_color" android:gravity="center" android:id="@+id/wallet_account_appply_submit_btn" android:layout_width="match_parent" android:layout_height="@dimen/wallet_dp_52" android:layout_marginTop="@dimen/wallet_dp_9" android:layout_marginBottom="@dimen/wallet_dp_9" android:text="@string/Fintech_Payment_Optimization_Next_step_SRDJ" style="@style/wallet_kyc_confirm_button_style"/>
        </LinearLayout>
    </RelativeLayout>
</FrameLayout>
