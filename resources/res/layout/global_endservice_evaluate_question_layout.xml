<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@drawable/global_evaluate_question_dialog_bg" android:layout_width="match_parent" android:layout_height="wrap_content">
    <FrameLayout android:id="@+id/global_endservice_evaluate_loading" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <LinearLayout android:orientation="horizontal" android:paddingLeft="18dp" android:paddingTop="18dp" android:paddingRight="18dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="20sp" android:textStyle="bold" android:textColor="@color/black" android:id="@+id/tv_global_unevaluate_title" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:layout_marginBottom="4dp" android:text="@string/afanty_loading_please_wait" android:includeFontPadding="false" android:layout_weight="1"/>
        <ImageView android:layout_gravity="top|right|center_vertical|center_horizontal|center|end" android:id="@+id/global_evaluate_question_close_button" android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/global_evaluate_question_close_button" android:contentDescription="@string/global_evaluate_question_close_button"/>
    </LinearLayout>
    <TextView android:textSize="14sp" android:textColor="@color/light_gray" android:ellipsize="end" android:id="@+id/tv_global_unevaluate_subtitle" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginRight="18dp" android:layout_marginBottom="14dp" android:maxLines="2" android:includeFontPadding="false"/>
    <TextView android:textSize="14sp" android:textColor="@color/g_color_666666" android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/tv_global_unevaluate_content" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginRight="18dp" android:layout_marginBottom="10dp" android:maxLines="2" android:includeFontPadding="false"/>
    <View android:id="@+id/global_unevaluate_question_content_splite" android:background="#ffececec" android:visibility="gone" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"/>
    <TextView android:textSize="16sp" android:textColor="@color/black" android:gravity="center_vertical" android:id="@+id/tv_global_unevaluate_question_content" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="15dp" android:layout_marginRight="18dp" android:layout_marginBottom="10dp" android:includeFontPadding="false"/>
    <FrameLayout android:gravity="center_vertical" android:id="@+id/fl_global_evaluate_container" android:layout_width="match_parent" android:layout_height="wrap_content"/>
</LinearLayout>
