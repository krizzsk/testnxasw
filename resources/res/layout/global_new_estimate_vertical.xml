<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@drawable/global_new_ui_estimate_vertical_bg" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="22dp">
        <com.didi.component.estimate.newui.view.NewEstimateMessageView android:id="@+id/new_estimate_guide_list" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <FrameLayout android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="-22dp">
            <androidx.constraintlayout.widget.ConstraintLayout android:layout_gravity="bottom" android:id="@+id/new_estimate_real_data_container" android:background="@drawable/global_new_ui_estimate_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginTop="22dp" android:layout_marginRight="10dp">
                <ImageView android:layout_gravity="center" android:id="@+id/bb_pull_up_view" android:paddingTop="5dp" android:layout_width="50dp" android:layout_height="@dimen/estimate_vertical_top_line_height" android:layout_marginTop="-10dp" android:src="@drawable/estimate_vertical_list_top_line_icon" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
                <Space android:id="@+id/space" android:layout_width="0dp" android:layout_height="0dp" app:layout_constraintBottom_toTopOf="0" app:layout_constraintStart_toStartOf="0"/>
                <androidx.recyclerview.widget.RecyclerView android:id="@+id/new_estimate_recommend_list" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/space"/>
                <androidx.recyclerview.widget.RecyclerView android:id="@+id/new_estimate_all_list" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/new_estimate_recommend_list"/>
                <View android:id="@+id/new_estimate_list_empty_view" android:background="@color/white" android:layout_width="match_parent" android:layout_height="500dp" app:layout_constraintTop_toBottomOf="@+id/new_estimate_all_list"/>
                <com.didi.component.estimate.newui.view.one.NewEstimateChooseOneCarView android:id="@+id/new_estimate_one_car_view" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintBottom_toTopOf="@+id/new_estimate_horizontal_operation_list" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="0"/>
                <com.didi.component.estimate.newui.view.CarEstimateOptionsView android:layout_gravity="bottom" android:id="@+id/new_estimate_horizontal_operation_list" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/new_estimate_one_car_view"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View android:id="@+id/new_estimate_magnifier" android:background="@drawable/new_estimate_vertical_item_shadow" android:layout_width="match_parent" android:layout_height="164dp" android:layout_marginLeft="-23dp" android:layout_marginRight="-23dp"/>
        </FrameLayout>
    </LinearLayout>
    <com.didi.component.estimate.newui.view.NewEstimateAbnormalView android:layout_gravity="bottom" android:id="@+id/new_estimate_abnormal" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/new_estimate_abnormal_view_height"/>
</FrameLayout>
