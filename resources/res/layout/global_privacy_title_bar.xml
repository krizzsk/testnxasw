<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/title_bar_container" android:layout_width="match_parent" android:layout_height="wrap_content">
    <FrameLayout android:id="@+id/img_left_container" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <FrameLayout android:id="@+id/img_left_small_container" android:paddingLeft="16dp" android:paddingTop="14dp" android:paddingBottom="14dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:paddingStart="16dp" android:paddingEnd="16dp">
            <ImageView android:id="@+id/img_left_small" android:background="@drawable/global_privacy_img_back_normal" android:layout_width="18dp" android:layout_height="18dp" android:layout_centerVertical="true"/>
        </FrameLayout>
        <ImageView android:id="@+id/img_left_large" android:layout_width="50dp" android:layout_height="46dp" android:src="@drawable/global_privacy_img_back_normal" android:scaleType="centerInside" android:layout_centerVertical="true"/>
    </FrameLayout>
    <TextView android:textSize="18sp" android:textColor="#000000" android:id="@+id/txt_title_mid" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="64dp" android:layout_marginRight="64dp" android:text="title" android:lines="1" android:layout_centerHorizontal="true" android:layout_centerVertical="true" android:layout_marginStart="64dp" android:layout_marginEnd="64dp"/>
    <TextView android:textSize="18sp" android:textStyle="bold" android:textColor="#000000" android:id="@+id/txt_title_left" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="14dp" android:layout_marginRight="64dp" android:text="title" android:lines="1" android:layout_toRightOf="@+id/img_left_container" android:layout_centerVertical="true" android:layout_marginStart="14dp" android:layout_marginEnd="64dp" android:layout_toEndOf="@+id/img_left_container"/>
</RelativeLayout>
