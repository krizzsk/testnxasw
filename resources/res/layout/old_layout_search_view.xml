<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/white" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp">
        <ImageView android:id="@+id/sug_close_btn" android:background="@drawable/old_selector_title_close" android:layout_width="20dp" android:layout_height="20dp" android:scaleType="centerInside" android:layout_centerVertical="true" android:contentDescription="@string/global_sug_close_button" android:layout_marginStart="18dp"/>
        <TextView android:textSize="18sp" android:textStyle="bold" android:textColor="#333333" android:id="@+id/set_common_title" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="common title" android:includeFontPadding="false" android:layout_centerInParent="true" style="@style/Map.TextViewStyle.TextDirection"/>
    </RelativeLayout>
    <com.didi.addressold.widget.EditTextErasable android:textSize="14sp" android:textColorHint="#33000000" android:id="@+id/et_start" android:background="@drawable/old_report_submit_more_button_selector" android:layout_width="match_parent" android:layout_height="34dp" android:layout_marginTop="12dp" android:hint="@string/global_sug_to_departure" android:singleLine="true" android:selectAllOnFocus="true" android:drawablePadding="10dp" android:inputType="textNoSuggestions" android:imeOptions="actionSearch" android:contentDescription="@string/global_sug_pick_up" android:textCursorDrawable="@drawable/old_poi_one_address_edittext_cursor" android:drawableStart="@drawable/old_report_green_dot" android:paddingStart="10dp" android:layout_marginStart="@dimen/common_margin" android:layout_marginEnd="@dimen/common_margin" style="@style/Map.EditTextStyle.Alignment"/>
    <RelativeLayout android:id="@+id/end_layout" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="12dp" android:layout_marginStart="@dimen/common_margin" android:layout_marginEnd="@dimen/common_margin">
        <com.didi.addressold.widget.EditTextErasable android:textSize="14sp" android:textColorHint="#33000000" android:id="@+id/et_end" android:background="@drawable/old_report_submit_more_button_selector" android:layout_width="match_parent" android:layout_height="34dp" android:hint="@string/global_sug_to_go" android:singleLine="true" android:selectAllOnFocus="true" android:drawablePadding="10dp" android:inputType="textNoSuggestions" android:imeOptions="actionSearch" android:contentDescription="@string/global_sug_destination" android:textCursorDrawable="@drawable/old_poi_one_address_edittext_cursor" android:drawableStart="@drawable/old_report_brown_dot" android:paddingStart="10dp" android:layout_toStartOf="@+id/enter_way_point" style="@style/Map.EditTextStyle.Alignment"/>
        <ImageView android:id="@+id/enter_way_point" android:visibility="gone" android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/poi_one_address_enter_way_point" android:scaleType="centerInside" android:layout_centerVertical="true" android:contentDescription="@string/enter_way_pointer_icon_label" android:layout_marginStart="12dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <com.didi.addressold.widget.EditTextErasable android:textSize="14sp" android:textColorHint="#33000000" android:id="@+id/et_common" android:background="@drawable/old_report_submit_more_button_selector" android:layout_width="match_parent" android:layout_height="34dp" android:layout_marginTop="12dp" android:singleLine="true" android:selectAllOnFocus="true" android:drawablePadding="10dp" android:inputType="textNoSuggestions" android:imeOptions="actionSearch" android:textCursorDrawable="@drawable/old_poi_one_address_edittext_cursor" android:paddingStart="10dp" android:layout_marginStart="@dimen/common_margin" android:layout_marginEnd="@dimen/common_margin" style="@style/Map.EditTextStyle.Alignment"/>
    <TextView android:focusable="true" android:focusableInTouchMode="true" android:layout_width="1px" android:layout_height="1px" style="@style/Map.TextViewStyle.TextDirection"/>
</LinearLayout>
