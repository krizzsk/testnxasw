<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.didi.dimina.starbox.p108ui.widget.TitleView android:layout_width="match_parent" android:layout_height="wrap_content" app:title="@string/dm_kit_launch"/>
    <LinearLayout android:orientation="horizontal" android:padding="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp">
        <TextView android:textSize="@dimen/font_size_medium" android:textColor="@color/dk_color_aa000000" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="0dp" android:layout_height="wrap_content" android:text="DMMinaID" android:layout_weight="0.5"/>
        <TextView android:textSize="@dimen/font_size_medium" android:textColor="@color/dk_color_aa000000" android:gravity="right|center_vertical|center_horizontal|center|end" android:id="@+id/tv_dmmina_id" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5"/>
    </LinearLayout>
    <TextView android:background="@color/dk_color_999999" android:padding="5dp" android:layout_width="match_parent" android:layout_height="0.4dp"/>
    <TextView android:textSize="@dimen/font_size_large" android:textColor="@color/dk_color_aa000000" android:padding="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:text="启动时间"/>
    <TextView android:background="@color/dk_color_999999" android:padding="5dp" android:layout_width="match_parent" android:layout_height="0.4dp"/>
    <LinearLayout android:orientation="horizontal" android:padding="5dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="@dimen/font_size_medium" android:textColor="@color/dk_color_aa000000" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="0dp" android:layout_height="wrap_content" android:text="jsCore加载时间" android:layout_weight="0.5"/>
        <TextView android:textSize="@dimen/font_size_medium" android:textColor="@color/dk_color_aa000000" android:gravity="right|center_vertical|center_horizontal|center|end" android:id="@+id/tv_engine_info" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5"/>
    </LinearLayout>
    <TextView android:background="@color/dk_color_999999" android:padding="5dp" android:layout_width="match_parent" android:layout_height="0.4dp"/>
    <LinearLayout android:orientation="horizontal" android:padding="5dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="@dimen/font_size_large" android:textColor="@color/dk_color_aa000000" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="0dp" android:layout_height="wrap_content" android:text="首次启动时间" android:layout_weight="0.5"/>
        <TextView android:textSize="@dimen/font_size_medium" android:textColor="@color/dk_color_aa000000" android:gravity="right|center_vertical|center_horizontal|center|end" android:id="@+id/tv_first_launch_info" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5"/>
    </LinearLayout>
    <TextView android:background="@color/dk_color_999999" android:padding="5dp" android:layout_width="match_parent" android:layout_height="0.4dp"/>
    <TextView android:textSize="@dimen/font_size_large" android:textColor="@color/dk_color_aa000000" android:padding="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:text="webview加载HTML时间"/>
    <TextView android:background="@color/dk_color_999999" android:padding="5dp" android:layout_width="match_parent" android:layout_height="0.4dp"/>
    <androidx.recyclerview.widget.RecyclerView android:id="@+id/rv_load_page_time" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="5dp"/>
</LinearLayout>
