<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:id="@+id/mask" android:background="@color/black_effective" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <Button android:id="@+id/back_btn" android:background="@mipmap/back" android:layout_width="20dp" android:layout_height="20dp" android:layout_marginLeft="20dp" android:layout_marginTop="60dp" android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
    <Button android:id="@+id/question_btn" android:background="@mipmap/question" android:layout_width="20dp" android:layout_height="20dp" android:layout_marginTop="60dp" android:layout_marginRight="20dp" android:layout_alignParentRight="true"/>
    <RelativeLayout android:orientation="horizontal" android:id="@+id/safe_alert" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="50dp" android:layout_marginTop="56dp" android:layout_marginRight="50dp" android:layout_centerHorizontal="true">
        <TextView android:textSize="12dp" android:textColor="@color/color_white_80" android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/alert_txt" android:background="@drawable/anquan_alert_selector" android:paddingLeft="40dp" android:paddingTop="6dp" android:paddingRight="12dp" android:paddingBottom="6dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:minHeight="23dp" android:text="@string/anquan_alert" android:maxLines="3"/>
        <ImageView android:id="@+id/alert_img" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="8dp" android:src="@mipmap/anquan_alert" android:scaleType="fitXY" android:minWidth="26dp" android:layout_centerVertical="true"/>
    </RelativeLayout>
    <TextView android:textSize="14sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/gps_weak" android:background="@color/black" android:padding="5dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="130dp" android:text="@string/gps_weak" android:maxLines="2" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
    <com.didi.hawaii.p120ar.core.p121zg.slidingup.SlidingUpPanelLayout android:gravity="bottom" android:id="@+id/sliding_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" app:umanoDragView="@+id/drag_btn" app:umanoFadeColor="@android:color/transparent" app:umanoInitialState="0" app:umanoOverlay="true" app:umanoPanelHeight="40dp" app:umanoParallaxOffset="0dp" app:umanoShadowHeight="0dp">
        <FrameLayout android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <FrameLayout android:orientation="vertical" android:id="@+id/drag_view" android:background="@android:color/transparent" android:layout_width="match_parent" android:layout_height="280dp">
            <Button android:layout_gravity="center_horizontal" android:id="@+id/drag_btn" android:background="@android:color/transparent" android:layout_width="170dp" android:layout_height="90dp"/>
            <LinearLayout android:layout_gravity="bottom" android:orientation="vertical" android:id="@+id/map_back_container" android:layout_width="match_parent" android:layout_height="240dp"/>
            <LinearLayout android:layout_gravity="bottom" android:orientation="vertical" android:id="@+id/map_container" android:layout_width="match_parent" android:layout_height="240dp"/>
            <ImageView android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="42dp" android:src="@mipmap/collapse_map_arrow"/>
        </FrameLayout>
    </com.didi.hawaii.p120ar.core.p121zg.slidingup.SlidingUpPanelLayout>
    <RelativeLayout android:id="@+id/expand_map" android:visibility="gone" android:layout_width="140dp" android:layout_height="80dp" android:layout_marginBottom="45dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true">
        <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="9dp" android:src="@mipmap/expand_map_arrow" android:layout_above="@+id/expand_txt" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="18dp" android:textStyle="bold" android:textColor="#fafeff" android:id="@+id/expand_txt" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/expand_map" android:maxLines="1" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</RelativeLayout>
