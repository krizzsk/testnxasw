<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="16sp" android:textColor="#555" android:id="@+id/tv_appid" android:layout_width="0dp" android:layout_height="wrap_content" android:text="APPID" android:layout_weight="1"/>
            <androidx.appcompat.widget.SwitchCompat android:id="@+id/tbn" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginStart="10dp"/>
        </LinearLayout>
        <TextView android:textSize="16sp" android:textColor="#555" android:id="@+id/tv_version" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:text="JSSDK版本"/>
    </LinearLayout>
    <View android:background="#efefef" android:layout_width="match_parent" android:layout_height="0.6dp"/>
</LinearLayout>
