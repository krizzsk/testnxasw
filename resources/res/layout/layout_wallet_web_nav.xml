<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="44dp">
    <TextView android:textSize="20sp" android:textColor="@color/wallet_color_000000" android:layout_gravity="center" android:id="@+id/tv_title" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <ImageView android:layout_gravity="center_vertical" android:id="@+id/iv_back" android:layout_width="16dp" android:layout_height="16dp" android:src="@drawable/ic_pay_secure_back" android:layout_marginStart="16dp"/>
    <ImageView android:layout_gravity="right|center_vertical|center_horizontal|center|end" android:id="@+id/iv_service" android:layout_width="17dp" android:layout_height="17dp" android:src="@drawable/wallet_service_enter_icon" android:layout_marginEnd="16dp"/>
    <ImageView android:layout_gravity="right|center_vertical|center_horizontal|center|end" android:id="@+id/iv_setting" android:layout_width="17dp" android:layout_height="17dp" android:src="@drawable/wallet_setting_entrance_icon" android:layout_marginEnd="56dp"/>
</FrameLayout>
