<?xml version="1.0" encoding="utf-8"?>
<com.didi.component.evaluate.EvaluateRootContainer xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/global_evaluate" android:background="@android:color/transparent" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:id="@+id/oc_evaluate_root_container" android:background="@drawable/global_evaluate_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="30dp">
        <FrameLayout android:id="@+id/oc_evaluate_loading" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <com.didi.component.evaluate.widget.CardTitleView android:id="@+id/oc_evaluate_title" android:layout_width="match_parent" android:layout_height="55dp"/>
        <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@+id/oc_evaluate_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_centerHorizontal="true">
            <View android:id="@+id/sv_top_line" android:background="@drawable/global_evaluate_line_top" android:visibility="gone" android:layout_width="match_parent" android:layout_height="4dp"/>
            <com.didi.component.evaluate.widget.ObservableScrollView android:id="@+id/oc_evaluate_scroll" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
                    <com.didi.component.evaluate.widget.IdentityView android:id="@+id/oc_evaluate_identity" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="28dp" android:layout_marginRight="28dp" android:layout_marginBottom="10dp"/>
                    <com.didi.component.evaluate.view.widget.FiveStarEvaluatedView android:id="@+id/oc_evaluate_five_star_view" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                    <TextView android:textSize="18dp" android:textStyle="bold" android:textColor="@color/g_color_000" android:gravity="center" android:layout_gravity="center" android:id="@+id/tv_evaluate_main" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp"/>
                    <TextView android:textSize="14dp" android:textColor="@color/g_color_666666" android:gravity="center" android:layout_gravity="center" android:id="@+id/tv_evaluate_sub" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp"/>
                    <TextView android:textSize="15sp" android:id="@+id/tv_tip" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="28dp" android:layout_marginRight="28dp" android:text="Have to tip"/>
                    <ViewStub android:id="@+id/oc_evaluate_tag_area_stub" android:layout="@layout/global_include_evaluate_tag_area" android:inflatedId="@+id/oc_evaluate_tag_view" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="28dp" android:layout_marginTop="11dp" android:layout_marginRight="28dp"/>
                    <ViewStub android:layout_gravity="center_horizontal" android:id="@+id/oc_evaluate_comment_area_stub" android:layout="@layout/global_evaluate_comment_view" android:inflatedId="@+id/oc_evaluate_comment_area" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="10dp"/>
                    <View android:id="@+id/comment_area_margin" android:visibility="gone" android:layout_width="match_parent" android:layout_height="10dp"/>
                </LinearLayout>
            </com.didi.component.evaluate.widget.ObservableScrollView>
            <View android:id="@+id/sv_bottom_line" android:background="@drawable/global_evaluate_evaluate_line_bottom" android:visibility="gone" android:layout_width="match_parent" android:layout_height="4dp"/>
            <com.didi.component.evaluate.view.TipView android:id="@+id/oc_evaluate_tip" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="28dp" android:layout_marginTop="17dp" android:layout_marginRight="28dp"/>
            <com.didi.component.evaluate.widget.SubmitView android:id="@+id/oc_evaluate_submit" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="18dp" android:layout_marginRight="18dp" android:layout_marginBottom="8dp"/>
            <RelativeLayout android:id="@+id/unmatch_layout" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:textSize="14sp" android:textColor="@color/g_color_333333" android:id="@+id/unmatch_detail" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="16dp" android:layout_marginBottom="19.5dp" android:text="@string/global_unmatch_evaluate_detail"/>
                <TextView android:textSize="14sp" android:textColor="@color/g_color_FF6F26" android:id="@+id/unmatch_submit" android:visibility="gone" android:clickable="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:layout_marginRight="18dp" android:layout_marginBottom="19.5dp" android:text="@string/global_unmatch_evaluate_submit" android:layout_alignParentRight="true"/>
                <TextView android:textSize="14sp" android:textColor="@color/g_color_333333" android:id="@+id/unmatch_result_evaluate" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="18dp" android:layout_marginTop="16dp" android:text="@string/global_unmatch_evaluate_result"/>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</com.didi.component.evaluate.EvaluateRootContainer>
