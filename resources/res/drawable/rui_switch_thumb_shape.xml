<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <size android:height="28dp" android:width="28dp"/>
        </shape>
    </item>
    <item android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp">
        <shape android:shape="oval">
            <solid android:color="@color/rui_color_white"/>
            <stroke android:width="0.5dp" android:color="@color/rui_color_grey_4"/>
            <size android:height="28dp" android:width="28dp"/>
        </shape>
    </item>
</layer-list>
