<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/safety_god_theme_color_trans"/>
            <stroke android:width="1dp" android:color="@color/safety_god_theme_color"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#e9e9e9"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
</selector>
