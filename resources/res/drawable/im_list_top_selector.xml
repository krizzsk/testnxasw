<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#eaeef7"/>
            <stroke android:width="0px" android:color="#f0f0f0" android:dashWidth="0dp" android:dashGap="0dp"/>
            <corners android:topLeftRadius="0dp" android:topRightRadius="0dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#f2f6ff" android:endColor="#f2f6ff" android:angle="90" android:type="linear" android:gradientRadius="90"/>
            <stroke android:width="0dp" android:color="#ffffff" android:dashWidth="0dp" android:dashGap="0dp"/>
            <corners android:topLeftRadius="0dp" android:topRightRadius="0dp"/>
        </shape>
    </item>
</selector>
