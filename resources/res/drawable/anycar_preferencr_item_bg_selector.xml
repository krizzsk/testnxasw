<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false" android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="18dp"/>
            <solid android:color="#f3f4f5"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="18dp"/>
            <solid android:color="#fff2ea"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="18dp"/>
            <solid android:color="#f8f9fa"/>
        </shape>
    </item>
</selector>
