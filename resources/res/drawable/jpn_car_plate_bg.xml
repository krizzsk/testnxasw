<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <stroke android:width="1dp" android:color="#d4d4d4"/>
            <corners android:radius="15dp"/>
        </shape>
    </item>
    <item android:left="8dp" android:top="8dp" android:right="8dp" android:bottom="8dp">
        <shape android:shape="rectangle">
            <solid android:color="#2fa186"/>
            <corners android:radius="11dp"/>
            <padding android:left="6dp" android:top="6dp" android:right="6dp" android:bottom="6dp"/>
        </shape>
    </item>
</layer-list>
