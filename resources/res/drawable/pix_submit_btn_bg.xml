<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <corners android:radius="45dp"/>
            <gradient android:startColor="#ddfff366" android:endColor="#ddffc040" android:angle="0"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <corners android:radius="45dp"/>
            <gradient android:startColor="#fff366" android:endColor="#ffc040" android:angle="0"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="45dp"/>
            <solid android:color="#f3f4f5"/>
        </shape>
    </item>
</selector>
