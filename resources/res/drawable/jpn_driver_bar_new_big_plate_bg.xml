<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <solid android:color="#12000000"/>
        </shape>
    </item>
    <item android:left="0.5dp" android:top="0.5dp" android:right="0.5dp" android:bottom="0.5dp">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <solid android:color="#ffffffff"/>
        </shape>
    </item>
    <item android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp">
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <solid android:color="#4da691"/>
        </shape>
    </item>
</layer-list>
