<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#1e000000"/>
            <solid android:color="#ffffffff"/>
            <corners android:radius="75dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#fffed731"/>
            <solid android:color="#fffffaed"/>
            <corners android:radius="75dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#fffed731"/>
            <solid android:color="#fffffaed"/>
            <corners android:radius="75dp"/>
        </shape>
    </item>
</selector>
