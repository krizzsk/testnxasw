<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/transparent"/>
            <stroke android:width="1dp" android:color="@color/transparent"/>
            <size android:height="5dp" android:width="5dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/light_sss_gray"/>
            <stroke android:width="1dp" android:color="#e0e0e0"/>
            <size android:height="5dp" android:width="5dp"/>
        </shape>
    </item>
</selector>
