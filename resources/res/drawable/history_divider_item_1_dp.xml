<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <size android:height="0.5dp" android:width="10dp"/>
            <solid android:color="@color/traffic_history_bg"/>
        </shape>
    </item>
    <item>
        <inset android:insetLeft="10dp">
            <shape>
                <size android:height="0.5dp" android:width="20dp"/>
                <solid android:color="@color/white"/>
            </shape>
        </inset>
    </item>
    <item>
        <inset android:insetLeft="20dp">
            <shape>
                <size android:height="0.5dp"/>
                <solid android:color="@color/traffic_history_bg"/>
            </shape>
        </inset>
    </item>
</layer-list>
