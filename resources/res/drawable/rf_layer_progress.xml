<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/rf_dimen_12"/>
            <solid android:color="@color/rf_color_gery_5_90_E5E5E5"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/rf_dimen_12"/>
                <solid android:color="@color/rf_color_brands_1_100"/>
            </shape>
        </scale>
    </item>
    <item android:id="@android:id/progress">
        <scale android:drawable="@drawable/rf_shape_progress_ct" android:scaleWidth="100%"/>
    </item>
</layer-list>
