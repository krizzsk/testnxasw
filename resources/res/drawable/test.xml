<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <size android:height="40dp" android:width="100dp"/>
            <solid android:color="#5eb888"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item android:top="-40dp" android:right="-30dp" android:bottom="65dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#ffffff"/>
            </shape>
        </rotate>
    </item>
    <item android:top="65dp" android:right="-30dp" android:bottom="-40dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#ffffff"/>
            </shape>
        </rotate>
    </item>
</layer-list>
